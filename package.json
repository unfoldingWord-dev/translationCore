    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.9.0",
    "eslint-plugin-jest": "22.1.2",
    "eslint-plugin-react": "7.7.0",
    "fetch-mock": "5.13.1",
    "find-duplicate-dependencies": "^2.0.1",
    "glob": "7.1.2",
    "gulp": "4.0.0",
    "gulp-change": "1.0.0",
    "jest": "21.2.1",
    "jest-enzyme": "4.2.0",
