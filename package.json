{
  "name": "translationCore",
  "productName": "translationCore",
  "version": "2.2.0",
  "minCompatibleVersion": "2.1.0",
  "manifestVersion": "7",
  "description": "A bridge between TS and TM",
  "main": "main.js",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "npm run react-build && copyfiles package.json build/ && npm run electron-build",
    "electron-start": "electron .",
    "electron-build": "electron-packager ./build --out=dist",
    "react-start": "craco start",
    "react-build": "craco build",
    "start": "concurrently \"npm run react-start\" \"wait-on http://localhost:3000/ && cross-env NODE_ENV=development npm run electron-start\"",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test-one": "craco test",
    "test": "eslint ./stories && craco test --testPathIgnorePatterns=src/tC_apps/ --logHeapUsage",
    "ci-test": "craco test --max-old-space-size=3072 --maxWorkers=4 --testPathIgnorePatterns=src/tC_apps/ --logHeapUsage --coverage",
    "find:extra-locale": "node scripts/locale/find_extras.js src/locale/English-en_US.json src/js",
    "find:missing-locale": "node scripts/locale/find_missing.js src/locale/English-en_US.json src/js",
    "locale-key-comparison": "node ./scripts/locale_key_comparison.js",
    "npm-clean-up": "rm -rf node_modules/ && rm -rf src/tC_apps/*/node_modules && rm package-lock.json && npm i",
    "release-content-update": "node scripts/releaseContentUpdate",
    "load-apps": "git submodule update --init --recursive",
    "overwrite-resources": "rm -rf tcResources/en/bibles tcResources/en/translationHelps tcResources/el-x-koine tcResources/hi && npm run update-resources",
    "update-apps": "git submodule foreach --recursive git checkout develop && git submodule foreach --recursive git pull",
    "update-attributions": "node scripts/attributions/generateAttributionData.js src/js/components/home/license/attributionData.json",
    "update-resources": "node scripts/resources/updateResources.js tcResources en hi el-x-koine hbo",
    "update-version": "node ./scripts/versionScript.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klappy/translationCore.git"
  },
  "keywords": [
    "Translation",
    "Bible",
    "DSM",
    "Wycliffe",
    "unfoldingWord"
  ],
  "author": {
    "name": "unfoldingWord",
    "email": "info@unfoldingword.org"
  },
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/unfoldingWord-dev/translationCore/issues"
  },
  "homepage": "https://github.com/unfoldingWord-dev/translationCore#readme",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/runtime": "7.8.7",
    "@storybook/react": "5.3.10",
